<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.metacog"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="9" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.metacog.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.metacog.SerieActivity"
            android:label="@string/title_activity_serie" >
        </activity>
        <activity
            android:name="com.example.metacog.QuestionActivity"
            android:label="@string/title_activity_question" >
        </activity>
        <activity
            android:name="com.example.metacog.AdminActivity"
            android:label="@string/title_activity_admin" >
        </activity>
        <activity
            android:name="com.example.metacog.AdminModuleActivity"
            android:label="@string/title_activity_admin_module" >
        </activity>
        <activity
            android:name="com.example.metacog.AdminSerieActivity"
            android:label="@string/title_activity_admin_serie" >
        </activity>
        <activity android:name="com.example.metacog.LogActivity" >
        </activity>
        <activity
            android:name="com.example.metacog.ModulesActivity"
            android:label="@string/title_activity_modules" >
        </activity>
        <activity
            android:name="com.example.metacog.EndExerciceActivity"
            android:label="@string/title_activity_end_exercice" >
        </activity>
    </application>

</manifest>
